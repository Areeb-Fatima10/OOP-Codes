abstract class Animal{
    abstract void sound();
    public void sleep(){
        System.out.println("Animal is Sleeping");
    }
}
class Dog extends Animal{
    @Override
    void sound(){
       System.out.println("Dog barks");  
    }
}
class Cat extends Animal{
    @Override
     void sound(){
       System.out.println("Cat Meows");  
    }
}
public class Main{
    public static void main(String[]args){
        Animal d = new Dog();
        Animal c = new Cat();
        d.sound();
        c.sound();
        
    }
}
